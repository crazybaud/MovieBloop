extends layout

block content
  h1= title
  p Welcome to #{title}
  p Search parameters note:#{note} date:#{date}
  input(type="text", name="dateMin",id="dateMin")
  input(type="text", name="dateMax",id="dateMax")
  #green
  script(type='text/javascript')
    // code a passer ensuite dans un fichier !!!
    $(function(){
      $('#dateMax').datepicker();
      $('#dateMin').datepicker();
      $('#green').slider({
        range: true,
        min: 0,
        max: 10,
        values: [ 6.7, 10 ],
        step: 0.1
        change: function(event, ui) {
          alert( "The value has changed to"+ui.value);
        }
      });
    });
  //- $( "#elem" ).progressbar( "disable" );